          "group": "favoritesgroup@1"
        },
        {
          "command": "SAS.server.renameResource",
          "when": "viewItem =~ /update/ && view == serverdataprovider && !listMultiSelection",
          "group": "delrenamegroup@0"
        },
        {
          "command": "SAS.server.deleteResource",
          "when": "viewItem =~ /delete/ && view == serverdataprovider",
          "group": "delrenamegroup@1"
        },
        {
          "command": "SAS.server.convertNotebookToFlow",
