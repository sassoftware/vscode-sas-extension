    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.8",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/l10n-dev": "^0.0.35",
    "buffer": "6.0.3",
    "chai": "^4.4.1",
    "concurrently": "^9.1.0",
    "esbuild": "^0.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^7.2.0",
    "mocha": "^11.0.1",
    "nock": "^13.5.5",
    "papaparse": "^5.4.1",
    "path-browserify": "^1.0.1",
    "prettier": "^3.3.3",
    "sinon": "^19.0.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-sinon": "^2.0.2",
    "typescript": "^5.5.3",
