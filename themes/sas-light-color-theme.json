{
  "name": "SAS Light",
  "include": "./light_plus.json",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "sep": { "foreground": "#1b1d22" },
    "keyword": { "foreground": "#3578c5" },
    "sec-keyword": { "foreground": "#224c7c", "bold": true },
    "proc-name": { "foreground": "#224c7c", "bold": true },
    "comment": { "foreground": "#65802a" },
    "macro-keyword": { "foreground": "#3578c5" },
    "macro-comment": { "foreground": "#65802a" },
    "macro-ref": { "foreground": "#1b1d22", "bold": true },
    "macro-sec-keyword": { "foreground": "#1b1d22", "bold": true },
    "cards-data": { "foreground": "#ad6531" },
    "string": { "foreground": "#8f4238" },
    "date": { "foreground": "#3c8275", "bold": true },
    "time": { "foreground": "#3c8275", "bold": true },
    "dt": { "foreground": "#3c8275", "bold": true },
    "bitmask": { "foreground": "#3c8275", "bold": true },
    "namelit": { "foreground": "#8f4238", "bold": true },
    "hex": { "foreground": "#3c8275", "bold": true },
    "numeric": { "foreground": "#3c8275", "bold": true },
    "format": { "foreground": "#3c8275" },
    "error": { "foreground": "#ff0000" },
    "warning": { "foreground": "#8f5f00" },
    "note": { "foreground": "#0000ff" }
  }
}
